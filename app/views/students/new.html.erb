<h1>Student Form</h1>

<%= form_tag students_path do %>


<br>
  <form action="<%= students_path %>" method="POST">
    <label>First name:</label><br>
    <input type="text" id="first_name" name="student[first_name]"><br>

    <label>Second name:</label><br>
    <input type="text" id="last_name" name="student[last_name]"></input><br>

    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="submit" value="Submit Student">
  </form>

<% end %>
